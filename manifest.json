{
  "manifest_version": 3,
  "name": "Medicus Helper",
  "version": "1.0",
  "description": "Bo autosugestie sÄ… przyjemne",
  "permissions": [
    "activeTab",
    "scripting"
  ],
  "background": {
    "service_worker": "background.js"
  },
  "action": {
    "default_popup": "popup.html",
    "default_icon": {
      "16": "images/icon16.jpeg",
      "48": "images/icon48.jpeg",
      "128": "images/icon128.jpeg"
    }
  },
  "content_scripts": [
    {
      "matches": ["<all_urls>"],
      "js": ["contentScript.js"]
    }
  ],
  "icons": {
    "16": "icon16.jpeg",
    "19": "icon19.jpeg",
    "32": "icon32.jpeg",
    "38": "icon38.jpeg",
    "48": "icon48.jpeg",
    "128": "icon128.jpeg"
  }
}
